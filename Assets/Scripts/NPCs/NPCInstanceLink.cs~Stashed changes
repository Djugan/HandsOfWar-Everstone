using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class NPCInstanceLink {

	public bool active;
	public NPCData sourceData;
	public NPCManager instanceManager;
	public int zone;

	public NPCInstanceLink (bool _active, NPCData _sourceData, int _zone) {
		active = _active;
		sourceData = _sourceData;
		zone = _zone;
	}
}
